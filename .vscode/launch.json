  "configurations": [
    {
      "name": "Python 调试程序: 包含参数的当前文件",
      "type": "debugpy",
      "request": "launch",
      // "program": "models/semantic.py",
      "program": "train_ParCo_vq_lgvq.py",
      "console": "integratedTerminal",
      "args": [
        "--out-dir=output",
        "--exp-name=ParCo",
        "--dataname=t2m",
        "--batch-size=2",
        "--lr=2e-4",
        "--total-iter=300000",
        "--lr-scheduler=200000",
        "--vqvae-cfg=default",
        "--down-t=2",
        "--depth=3",
        "--dilation-growth-rate=3",
        "--vq-act=relu",
        "--quantizer=ema_reset",
        "--loss-vel=0.5",
        "--recons-loss=l1_smooth"
      ],
      "justMyCode": false,
      "env": {
        "PYTHONPATH": "${workspaceFolder}/..:${env:PYTHONPATH}",
        "TORCH_DISTRIBUTED_DEBUG": "INFO",
        "CUDA_VISIBLE_DEVICES": "0",
        "TOKENIZERS_PARALLELISM": "false"
      }
    }
  ]
